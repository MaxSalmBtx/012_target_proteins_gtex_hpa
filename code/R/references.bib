}

@Article{Fang2025,
  author   = {Fang, Huaying and Jiang, Lihua and da Veiga Leprevost, Felipe and Jian, Ruiqi and Chan, Joanne and Glinos, Dafni and Lappalainen, Tuuli and Nesvizhskii, Alexey I. and Reiner, Alexander P. and Consortium, GTEx and Snyder, Michael P. and Tang, Hua},
