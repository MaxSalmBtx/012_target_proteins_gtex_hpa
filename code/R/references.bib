  keywords        = {Brain, metabolism, pathology; Brain Neoplasms, metabolism; Data Analysis; Glioma, metabolism; Humans; Information Storage and Retrieval; Neoplasm Proteins, metabolism; Software; Human protein atlas; Proteomics; Software; Visualization},
  nlm-id          = {100965194},
