  issue           = {3},
  keywords        = {Humans; Proteome, analysis; Proteomics, methods; Gene Expression Profiling; Databases, Factual; Mass Spectrometry, methods; Databases, Protein; human proteome; mass spectrometry; public data re-use; quantitative proteomics},
  nlm-id          = {101128775},
  owner           = {NLM},
